{
  "name": "api-tests",
  "packageManager": "yarn@4.4.1",
  "type": "module",
  "volta": {
    "extends": "../../package.json"
  },
  "scripts": {
    "dev": "test -f src/graphql-env.d.ts || yarn generate && wait-port 4000 && TEST_PASSWORD=a vitest",
