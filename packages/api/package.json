  "imports": {
    "#lib": "./build/src/lib/index.js",
    "#modules/*": "./build/src/modules/*/index.js",
    "#permissions": "./build/src/permissions/index.js"
  },
  "private": true,
  "scripts": {
    "format-at": "prettier --write --ignore-unknown",
    "format": "yarn format-at .",
    "build": "run-s \"build:*\"",
    "prisma": "yarn workspace @churros/db run prisma",
    "build:prisma": "yarn workspace @churros/db run generate",
    "build:changelog": "run-s copy-changelog",
    "build:typenames": "tsx ./scripts/update-id-prefix-to-typename-map.ts",
    "build:barrelize": "run-s barrelize",
    "build:api": "tsc",
    "build:post": "tsx ./src/post-build.ts",
    "check": "yarn build",
    "copy-changelog": "cp ../../CHANGELOG.md ./static/CHANGELOG.md",
    "clean": "git clean -Xdf .",
    "dev": "yarn tsx ./scripts/update-mail-templates-type.ts && yarn build:changelog && yarn barrelize && yarn run -T generate-buildinfo --stub && NODE_ENV=development tsx watch --conditions=development --clear-screen=false ./src/index.ts",
    "migrate-old-data": "cd scripts && yarn tsc import-old-data.ts --lib es2021,DOM --module es2022 --target es2022 --moduleResolution node --allowSyntheticDefaultImports && yarn node import-old-data.js",
    "start": "node ./build/src/index.js",
    "barrelize": "tsx scripts/generate-barrelsby-config.ts && echo Running barrelsby && barrelsby --config barrelsby.config.json && tsx scripts/fix-extensionless-barrel-imports.ts && echo Formatting barrels for consistency && yarn format-at ./src/modules/**/index.ts --log-level silent",
    "new-module": "tsx ./scripts/new-module.ts",
    "add-search-support": "tsx ./scripts/add-search-support.ts",
    "modules-import-graph": "tsx ./scripts/modules-import-graph.ts --exclude global --png",
    "migration": "yarn run -T migration",
    "watch-barrels": "nodemon --exec 'yarn barrelize' --watch src/modules -e ts --ignore 'src/modules/**/index.ts'"
  },
  "type": "module",
  "volta": {
    "extends": "../../package.json"
  },
  "release-it": {
    "git": false,
    "npm": false
  }
